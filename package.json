    "postcss": "^7.0.14",
    "postcss-comment": "^2.0.0",
    "postcss-custom-media": "^7.0.8",
    "postcss-extend": "^1.0.5",
    "postcss-for": "^2.1.1",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.1.2",
    "prettier": "^1.16.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ademilter/tasarim-mutfagi-web"
  },
  "bugs": {
    "url": "https://github.com/ademilter/tasarim-mutfagi-web/issues"
