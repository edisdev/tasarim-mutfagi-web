    "gatsby-transformer-json": "^2.1.11",
    "gatsby-transformer-remark": "^2.3.12",
    "gatsby-transformer-sharp": "^2.1.17",
    "netlify-cms": "^2.9.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-flickity-component": "^3.2.0",
    "react-helmet": "^5.2.0",
    "react-use-scroll-position": "^2.0.0"
